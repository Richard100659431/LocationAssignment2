<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1CB59F"
    tools:context=".CoordinateFragment">

    <Button
        android:id="@+id/updateButton"
        android:layout_width="118dp"
        android:layout_height="61dp"
        android:layout_marginTop="514dp"
        android:layout_marginEnd="28dp"
        android:backgroundTint="#FF32C819"
        android:text="Update Location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/addButton"
        android:layout_width="118dp"
        android:layout_height="61dp"
        android:layout_marginTop="418dp"
        android:layout_marginEnd="28dp"
        android:backgroundTint="#FF32C819"
        android:text="Add Location"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/deleteButton"
        android:layout_width="118dp"
        android:layout_height="61dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="514dp"
        android:backgroundTint="#FF32C819"
        android:text="Remove Location"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="132dp"
        android:layout_height="18dp"
        android:layout_marginStart="250dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="510dp"
        android:text="Input Longitude"
        android:textAlignment="center"
        android:textColor="#BD0000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.926" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="132dp"
        android:layout_height="18dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="510dp"
        android:text="Input Latitude"
        android:textAlignment="center"
        android:textColor="#BD0000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.926" />

    <EditText
        android:id="@+id/address"
        android:layout_width="219dp"
        android:layout_height="48dp"
        android:layout_marginStart="95dp"
        android:layout_marginEnd="236dp"
        android:layout_marginBottom="120dp"
        android:background="#DDDAB8"
        android:ems="10"
        android:gravity="center"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appNameLabel"
        app:layout_constraintVertical_bias="0.504"
        tools:ignore="SpeakableTextPresentCheck" />

    <EditText
        android:id="@+id/lat_input_bar"
        android:layout_width="159dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="236dp"
        android:layout_marginBottom="370dp"
        android:background="#DDDAB8"
        android:ems="10"
        android:gravity="center"
        android:hint="Input Latitude"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appNameLabel"
        app:layout_constraintVertical_bias="0.454"
        tools:ignore="DuplicateSpeakableTextCheck" />

    <FrameLayout
        android:id="@+id/constraintLayout"
        android:layout_width="408dp"
        android:layout_height="65dp"
        android:background="#32C819"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </FrameLayout>

    <TextView
        android:id="@+id/appNameLabel"
        android:layout_width="235dp"
        android:layout_height="36dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="83dp"
        android:text="Find A Location"
        android:textAlignment="center"
        android:textColor="#BD0000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout" />

    <TextView
        android:id="@+id/address_label"
        android:layout_width="246dp"
        android:layout_height="35dp"
        android:layout_marginStart="82dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="83dp"
        android:layout_marginBottom="250dp"
        android:text="Address"
        android:textAlignment="center"
        android:textColor="#BD0000"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
        app:layout_constraintVertical_bias="0.554" />

    <EditText
        android:id="@+id/long_input_bar"
        android:layout_width="159dp"
        android:layout_height="48dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="400dp"
        android:background="#DDDAB8"
        android:ems="10"
        android:gravity="center"
        android:hint="Input Longitude"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.973"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/appNameLabel"
        app:layout_constraintVertical_bias="0.238" />

    <Button
        android:id="@+id/enterCoords"
        android:layout_width="178dp"
        android:layout_height="57dp"
        android:layout_marginStart="124dp"
        android:layout_marginTop="600dp"
        android:backgroundTint="#FF32C819"
        android:text="Change Address"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/findButton"
        android:layout_width="118dp"
        android:layout_height="61dp"
        android:layout_marginStart="28dp"
        android:layout_marginTop="418dp"
        android:backgroundTint="#FF32C819"
        android:text="Get Address"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>